<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">

	
	<Dialog 
    	id="idMessageDialog3"
    	title="Guardar"
    	afterClose="">
	    <content>
		    <HBox    	
	    		alignItems="Center"
				justifyContent="Center">
				<Text text="Se habilitarán campos para su modificación" width="80%"/>
		    </HBox>    	
	    </content>
	    
	    <beginButton>
        	<Button text="Aceptar" press="acceptDialogAcceptSave" />
	    </beginButton>
	    <endButton>
        	<Button text="Cancelar" press="closeDialogCancelSave" />
	    </endButton>
    </Dialog>
</core:FragmentDefinition>